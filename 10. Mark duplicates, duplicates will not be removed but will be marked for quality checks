#!/bin/bash
#SBATCH -n 8
#SBATCH --job-name=markdups
#SBATCH -t 2-0:0
#SBATCH -A mwilsons
#SBATCH -o slurm.markdups.out
#SBATCH -e slurm.markdups.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=kcolney@asu.edu
#--------------------------

#-------------------------- 
# 1. start timer 
#-------------------------- 
date

#-------------------------- 
# 2. move to directory and load programs 
#-------------------------- 

cd /home/kcolney/cancer_HaynesLab/
module load bamtools/2.4.0 
module load java/latest

#-------------------------- 
# 3. BAMtools sort, mark duplicates 
#--------------------------
java -Xmx8g -jar /home/kcolney/ngcchome/tools2/picard.jar MarkDuplicates INPUT=KH1_S1_L001.sort.bam OUTPUT=KH1_S1_L001.sort.markdup.bam METRICS_FILE=KH1_S1_L001sort.markdup.picardMetrics.txt REMOVE_DUPLICATES=false ASSUME_SORTED=true VALIDATION_STRINGENCY=LENIENT
