#!/bin/bash
#SBATCH -n 8
#SBATCH --job-name=bamtools
#SBATCH -t 2-0:0
#SBATCH -A mwilsons
#SBATCH -o slurm.bamtools.out
#SBATCH -e slurm.bamtools.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=kcolney@asu.edu
#--------------------------

#-------------------------- 
# 1. start timer 
#-------------------------- 
date

#-------------------------- 
# 2. move to directory and load programs 
#-------------------------- 

cd /home/kcolney/cancer_HaynesLab/
module load bamtools/2.4.0 
module load java/latest

#-------------------------- 
# 3. BAMtools sort, mark duplicates 
#--------------------------
bamtools sort -in /scratch/kcolney/cancerCellLines_Haynes/2_data/STAR_pass1/KH1_S1_L001_pass1.Aligned.out.bam -out /scratch/kcolney/cancerCellLines_Haynes/2_data/BAM/KH1_S1_L001.sort.bam
java -Xmx8g -jar /home/kcolney/ngcchome/tools2/picard.jar MarkDuplicates INPUT=KH1_S1_L001.sorted.bam OUTPUT=KH1_S1_L001.sort.markdup.bam METRICS_FILE=KH1_S1_L001sort.markdup.picardMetrics.txt REMOVE_DUPLICATES=false ASSUME_SORTED=true VALIDATION_STRINGENCY=LENIENT
bamtools sort -in /scratch/kcolney/cancerCellLines_Haynes/2_data/STAR_pass1/KH1_S1_L002_pass1.Aligned.out.bam -out /scratch/kcolney/cancerCellLines_Haynes/2_data/BAM/KH1_S1_L002.sort.bam
