#!/bin/bash
#SBATCH -n 8
#SBATCH --job-name=Bamtools_addReadGroups
#SBATCH -t 0-10:0
#SBATCH -A mwilsons
#SBATCH -o slurm.Bamtools_addReadGroups.out
#SBATCH -e slurm.Bamtools_addReadGroups.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=kcolney@asu.edu
#--------------------------

#-------------------------- 
# 1. start timer 
#-------------------------- 
date

#-------------------------- 
# 2. move to directory and load programs 
#-------------------------- 

